    	<script src="/javascripts/vendor/what-input.min.js"></script>
    	<script src="/javascripts/foundation.js"></script>
    	<% header %>
    	<meta class="foundation-mq">
    </head>
    <body>
		<div class="title-bar" data-responsive-toggle="main-menu" data-hide-for="medium">
